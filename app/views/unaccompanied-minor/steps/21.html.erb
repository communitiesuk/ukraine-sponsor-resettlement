<div class="govuk-grid-row">
  <div class="govuk-grid-column-two-thirds">

    <a href="#" onclick="history.go(-1);return false;" class="govuk-back-link">Back</a>

    <%= form_for @application, url: "/unaccompanied-minor/steps/21", method: :post do |f| %>
      <%= f.govuk_collection_select :nationality, [OpenStruct.new(id: 1, name: 'English')], :id, :name, legend: legend: { text: t('sponsor_other_nationality.full', scope: "unaccompanied_minor.questions"), size: 'm' } %>
