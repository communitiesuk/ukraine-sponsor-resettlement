<div class="govuk-grid-row">
  <div class="govuk-grid-column-two-thirds">

    <a href="#" onclick="history.go(-1);return false;" class="govuk-back-link">Back</a>
    <%= form_for @application, url: "/sponsor-a-child/steps/11", method: :post do |f| %>  
  
    <%= f.govuk_error_summary link_base_errors_to: :has_other_names %>

  
    <h1 class="govuk-heading-l"><%= t('other_names.full', scope: "unaccompanied_minor.questions")%></h1>  

      <%= f.govuk_radio_buttons_fieldset(:has_other_names,
          legend: { size: 'm', text: "", hidden: true },
          hint: { text: t('other_names.hint', scope: "unaccompanied_minor.questions")}) do %>
        <%= f.govuk_radio_button :has_other_names, true, label: { text: t('Yes' ) } %>
        <%= f.govuk_radio_button :has_other_names, false, label: { text: t('No' ) },  link_errors: true  %>
      <% end %>

      <%= f.govuk_submit t('continue') %>
      <a class="govuk-link govuk-!-font-size-19" href="/sponsor-a-child/save-and-return">Save and return later</a>
    <% end %>
  </div>
</div>
